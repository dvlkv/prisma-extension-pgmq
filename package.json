{
  "name": "prisma-pgmq",
  "version": "1.0.0",
  "description": "A Prisma Client extension for PostgreSQL Message Queue (PGMQ) providing type-safe message queue operations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm build:watch",
    "prepublishOnly": "pnpm build && pnpm test",
    "clean": "rimraf dist"
  },
  "keywords": [
    "prisma",
    "postgresql",
    "message-queue",
    "pgmq",
    "queue",
    "background-jobs",
    "database",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/prisma-pgmq.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/prisma-pgmq/issues"
  },
  "homepage": "https://github.com/your-username/prisma-pgmq#readme",
  "peerDependencies": {
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}