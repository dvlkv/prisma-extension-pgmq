{
  "name": "prisma-extension-pgmq",
  "version": "0.1.0",
  "description": "A Prisma Client extension for PostgreSQL Message Queue (PGMQ) providing type-safe message queue operations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "eslint --ext .ts,.tsx",
    "lint:fix": "eslint --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm build:watch",
    "prepublishOnly": "pnpm lint && pnpm build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "prisma",
    "postgresql",
    "message-queue",
    "pgmq",
    "queue",
    "background-jobs",
    "database",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/prisma-extension-pgmq.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/prisma-extension-pgmq/issues"
  },
  "homepage": "https://github.com/your-username/prisma-extension-pgmq#readme",
  "peerDependencies": {
    "@prisma/client": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@prisma/client": "^5.22.0",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.57.1",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
