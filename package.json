{
  "name": "prisma-pgmq",
  "version": "0.1.0",
  "description": "A Prisma PGMQ implementation providing type-safe message queue operations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "eslint --ext .ts,.tsx",
    "lint:fix": "eslint --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm build:watch",
    "prepublishOnly": "pnpm lint && pnpm build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "prisma",
    "postgresql",
    "message-queue",
    "pgmq",
    "queue",
    "background-jobs",
    "database",
    "typescript"
  ],
  "author": "Dan Volkov <volkv.dan@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvlkv/prisma-pgmq.git"
  },
  "bugs": {
    "url": "https://github.com/dvlkv/prisma-pgmq/issues"
  },
  "homepage": "https://github.com/dvlkv/prisma-pgmq#readme",
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@prisma/client": "^5.0.0",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "prisma": "^5.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
